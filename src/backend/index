const sdk = require("api")("@affirm-developers/v2.0#lra732k0ljrje2ac");

sdk.auth("xcfbvdfgb", "ghjtyhj");
sdk
  .authorize_transaction(
    {
      expand: "checkout",
      order_id: 100095646,
      reference_id: 202307011027,
      transaction_id: "0KYZ5FN5JLPYG6IV",
    },
    { accept: "*/*" }
  )
  .then(({ data }) => console.log(data))
  .catch((err) => console.error(err));
